<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tictac">

    <application
        android:allowBackup="true"
        android:icon="@drawable/secound_xo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/secound_xo"
        android:supportsRtl="false"
        android:theme="@style/Theme.TicTac">

        <activity android:name=".WelcomeScreen" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".AddPlayers" android:exported="true"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"></activity>
    </application>

</manifest>